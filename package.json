{
    "name": "transredbatch",
    "version": "0.13",
    "title": "Red Batch",
    "description": "Red's alternative Batch Translator. Meant to be faster on bigger projects. Also able to prepare projects for batch translation. Git: https://github.com/reddo9999/transRedBatch",
    "main": "transredbatch.js",
    "icon": "icon.png",
    "author": {
        "name": "Reddo"
    },
    "autoload": false,
    "devDependencies": {
        "@types/jquery": "^3.5.13",
        "npm-pack-zip": "^1.3.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@types/node": "^17.0.16"
    },
    "scripts": {
        "build": "tsc && npm run pack",
        "pack": "npm-pack-zip --add-version --destination \"dist/\""
    },
    "load": [
        "transredbatch.js"
    ],
    "files": [
        "transredbatch.js",
        "transredbatch.d.ts",
        "README.MD",
        "icon.png",
        "LICENSE"
    ]
}
